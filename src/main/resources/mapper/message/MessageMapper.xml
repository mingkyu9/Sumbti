<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="app.sum.bti.message.mapper.MessageMapper">

    <select id="getMessageList" parameterType="map" resultType="messageList">

        select *
        from post_box,
             post_num,
             post_title,
             post_contents,
             post_time,
             user_id
    </select>




</mapper>